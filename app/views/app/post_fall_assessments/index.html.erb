<section class="content-header">
  <h1>
    Post Fall Assessments
  </h1>
</section>
<section class="content">
  <div class="box">
    <div class="box-header">
    <h3 class="box-title">Pain Assessments</h3>
      <div class="box-tools">
        <ul class="pull-right">
          <a href="<%= new_app_post_fall_assessment_path %>" class="btn btn-block btn-default btn-sm">New post fall assessment</a>
        </ul>
      </div>
    </div>
    <div class="box-body table-responsive no-padding">
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>Date</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @post_fall_assessment.each do |post_fall_assessment| %>
            <tr>
              <td><%= post_fall_assessment.date %></td>
              <td><%= link_to 'Show', app_post_fall_assessment_path(post_fall_assessment) %></td>
              <td><%= link_to 'Edit', edit_app_post_fall_assessment_path(post_fall_assessment) %></td>
              <td><%= link_to 'Destroy', app_post_fall_assessment_path(post_fall_assessment), method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <div class="text-right">
        <%= will_paginate @post_fall_assessment %>
      </div>
    </div>
  </div>
</section>
